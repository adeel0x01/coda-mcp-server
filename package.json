{
  "name": "coda-mcp-server",
  "version": "1.1.0",
  "description": "Model Context Protocol server for Coda API integration",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "coda-mcp": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "prepare": "npm run build",
    "dev": "tsc && node dist/index.js",
    "inspector": "tsc && npx @modelcontextprotocol/inspector dist/index.js"
  },
  "keywords": [
    "mcp",
    "coda",
    "api",
    "model-context-protocol"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adeel0x01/coda-mcp-server.git"
  },
  "bugs": {
    "url": "https://github.com/adeel0x01/coda-mcp-server/issues"
  },
  "homepage": "https://github.com/adeel0x01/coda-mcp-server#readme",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "zod": "^3.25.0",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
